{
  "manifest_version": 3,
  "name": "Youtube Red Bar",
  "version": "1.2",
  "description": "Change that youtube's bar to bring the full red back",
  "permissions": [],
  "icons": {
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "512": "icons/icon.png"
  },

  "content_scripts":[
    {
      "matches": [ "https://www.youtube.com/*" ],
      "js": [ "YTRB_Extension.js" ]
    }
  ]
}